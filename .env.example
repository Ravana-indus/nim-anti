# All Claude model requests are mapped to this model
MODEL="stepfun-ai/step-3.5-flash"


# Agent Config
CLAUDE_WORKSPACE="./agent_workspace"
MAX_CLI_SESSIONS=10
ALLOWED_DIR=""
FAST_PREFIX_DETECTION=true
ENABLE_NETWORK_PROBE_MOCK=true
ENABLE_TITLE_GENERATION_SKIP=true
ENABLE_SUGGESTION_MODE_SKIP=true
ENABLE_FILEPATH_EXTRACTION_MOCK=true


# Telegram Config
TELEGRAM_BOT_TOKEN=""
ALLOWED_TELEGRAM_USER_ID=""


# Messaging Rate Limiting
MESSAGING_RATE_LIMIT=1
MESSAGING_RATE_WINDOW=1


# NVIDIA NIM Config (Single key - for backward compatibility)
NVIDIA_NIM_API_KEY=""


# NVIDIA NIM Config (Multiple keys with rotation - comma-separated)
NVIDIA_NIM_API_KEYS="nvapi-key-1,nvapi-key-2,nvapi-key-3"


# Model Chain (Primary -> Fallback 1 -> Fallback 2 - comma-separated)
NVIDIA_NIM_MODEL_CHAIN="kimi-coding/k2p5,litellm-proxy/minimaxai/minimax-m2.1,litellm-proxy/z-ai/glm4.7"


# Rate Limiting (per-key)
NVIDIA_NIM_RATE_LIMIT=40
NVIDIA_NIM_RATE_WINDOW=60
NVIDIA_NIM_KEY_COOLDOWN_SECONDS=60


# Base URL is fixed to https://integrate.api.nvidia.com/v1
# All NVIDIA_NIM_* settings are strictly validated (unknown keys will error).

NVIDIA_NIM_TEMPERATURE=1.0
NVIDIA_NIM_TOP_P=1.0
NVIDIA_NIM_TOP_K=-1
NVIDIA_NIM_MAX_TOKENS=81920
NVIDIA_NIM_PRESENCE_PENALTY=0.0
NVIDIA_NIM_FREQUENCY_PENALTY=0.0
NVIDIA_NIM_MIN_P=0.0
NVIDIA_NIM_REPETITION_PENALTY=1.0
NVIDIA_NIM_SEED=
NVIDIA_NIM_STOP=
NVIDIA_NIM_PARALLEL_TOOL_CALLS=true
NVIDIA_NIM_RETURN_TOKENS_AS_TOKEN_IDS=false
NVIDIA_NIM_INCLUDE_STOP_STR_IN_OUTPUT=false
NVIDIA_NIM_IGNORE_EOS=false
NVIDIA_NIM_MIN_TOKENS=0
NVIDIA_NIM_CHAT_TEMPLATE=""
NVIDIA_NIM_REQUEST_ID=""
NVIDIA_NIM_REASONING_EFFORT=high
NVIDIA_NIM_INCLUDE_REASONING=true


# Server Port
PORT=8085
